{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
   "Jwt": {
    "Key": "${JWT_KEY}",
    "SecretKey": "${JWT_SECRET_KEY}",
    "Issuer": "PlaneTicketSystem",
    "Audience": "PlaneTicketSystemUsers",
    "TokenValidityInMinutes": 60,
    "RefreshTokenValidityInDays": 7
  },
  "AllowedHosts": "*"
}
